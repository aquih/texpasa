<?xml version="1.0" encoding="utf-8"?>
<odoo>
        
    <record id="view_move_form" model="ir.ui.view">
        <field name="name">account.move.form.inherit.texpasa</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <button name="button_draft" position="after">
                <button name="generar_diferencial" string="Generar Diferencial" type="object"/>
            </button>
        </field>
    </record>

    <record id="view_account_journal_form" model="ir.ui.view">
        <field name="name">account.journal.form.inherit.texpasa</field>
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form"/>
        <field name="arch" type="xml">
            <field name="country_code" position="before">
                <field name="compania_relacionada_id"/>
            </field>
        </field>
    </record>
    
</odoo>
